{
  "breadcrumbs.enabled": true,
  "atomKeymap.promptV3Features": true,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.cursorStyle": "block",
  "editor.formatOnPaste": false,
  "editor.formatOnSave": true,
  "editor.tabSize": 2,
  "editor.insertSpaces": true,
  "editor.detectIndentation": false,
  "workbench.startupEditor": "newUntitledFile",
  "workbench.editor.enablePreview": false,
  "explorer.confirmDragAndDrop": false,
  "editor.minimap.enabled": false,
  "editor.rulers": [80, 160],
  "editor.fontFamily": "FiraCode-Retina",
  "editor.fontLigatures": true,
  "gitlens.advanced.messages": {
    "suppressCommitHasNoPreviousCommitWarning": false,
    "suppressCommitNotFoundWarning": false,
    "suppressFileNotUnderSourceControlWarning": false,
    "suppressGitVersionWarning": false,
    "suppressLineUncommittedWarning": false,
    "suppressNoRepositoryWarning": false,
    "suppressResultsExplorerNotice": false,
    "suppressShowKeyBindingsNotice": true
  },
  "window.zoomLevel": 0,
  "ruby.lint": {
    "reek": true,
    "rubocop": {
      "only": ["SpaceInsideBlockBraces", "LeadingCommentSpace"],
      "lint": true,
      "rails": true
    },
    "ruby": {
      "unicode": true //Runs ruby -wc -Ku
    },
    "fasterer": true,
    "debride": true,
    "ruby-lint": true
  },
  "ruby.lintDebounceTime": 500,
  "ruby.codeCompletion": "rcodetools",
  "emmet.includeLanguages": {
    "erb": "html",
    "jsx": "html"
  },
  "workbench.iconTheme": "material-icon-theme",
  "explorer.confirmDelete": false,
  "[python]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 4,
    "editor.formatOnSave": false
  },
  "[json]": {
    "editor.formatOnSave": false
  },
  "[markdown]": {
    "editor.formatOnSave": false
  },
  "python.formatting.provider": "autopep8",
  "python.pythonPath": "/usr/local/bin/python3",
  "typescript.validate.enable": false,
  "javascript.validate.enable": false,
  "flow.pathToFlow": "/Users/matthiasjenny/LoanStreet/loanstreet-next/frontend/node_modules/.bin/flow",
  "workbench.colorTheme": "Night Owl (No Italics)",
  "vim.statusBarColorControl": true,
  "vim.statusBarColors.normal": ["#8FBCBB", "#434C5E"],
  "vim.statusBarColors.insert": "#BF616A",
  "vim.statusBarColors.visual": "#B48EAD",
  "vim.statusBarColors.visualline": "#B48EAD",
  "vim.statusBarColors.visualblock": "#A3BE8C",
  "vim.statusBarColors.replace": "#D08770",
  "workbench.colorCustomizations": {
    "statusBar.background": "#BF616A",
    "statusBar.noFolderBackground": "#BF616A",
    "statusBar.debuggingBackground": "#BF616A"
  }
}
